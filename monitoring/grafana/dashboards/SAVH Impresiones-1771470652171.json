          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH e AS (\r\n  SELECT\r\n    occurred_at,\r\n    from_status,\r\n    to_status,\r\n    lag(occurred_at) OVER (PARTITION BY job_id ORDER BY occurred_at) AS prev_at\r\n  FROM printing.print_job_status_events\r\n  WHERE (occurred_at AT TIME ZONE 'America/Santiago') BETWEEN $__timeFrom() AND $__timeTo()\r\n)\r\nSELECT\r\n  date_trunc('minute', occurred_at AT TIME ZONE 'America/Santiago') AS time,\r\n  'pending → generating' AS metric,\r\n  avg(extract(epoch from (occurred_at - prev_at))) AS value\r\nFROM e\r\nWHERE prev_at IS NOT NULL\r\n  AND from_status = 'pending'\r\n  AND to_status = 'generating'\r\nGROUP BY 1\r\nORDER BY 1;\r\n",
          "refId": "pending → generating (avg)",
